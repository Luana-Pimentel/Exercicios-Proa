//exercício 11

	//declaração de variáveis
	real numero1, numero2, numero3, numero4, numero5, numero6, numero7, numero8, numero9, numero10, media
	
	funcao inicio()
	{
		escreva("Bem vindo ao programa \n")
		escreva("Digite o primeiro valor: \n")
		leia(numero1)
		escreva("Digite o segundo valor: \n")
		leia(numero2)
		escreva("Digite o terceiro valor: \n")
		leia(numero3)
		escreva("Digite o quarto valor: \n")
		leia(numero4)
		escreva("Digite o quinto valor: \n")
		leia(numero5)
