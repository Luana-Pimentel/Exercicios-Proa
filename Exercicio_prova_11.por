		leia(numero9)
		escreva("Digite o décimo valor: \n")
		leia(numero10)
		limpa()

		media = (numero1 + numero2 + numero3 + numero4 + numero5 + numero6 + numero7 + numero8 + numero9 + numero10)/10

		escreva("Os números inseridos pelo usuário foram: \n", numero1, "\n", numero2, "\n", numero3, "\n", numero4, "\n", numero5, "\n", numero6, "\n", numero7, "\n", numero8, "\n", numero9, "\n", numero10, "\n", "E a média aritmética desses valores é ", media)
		
	}
}
